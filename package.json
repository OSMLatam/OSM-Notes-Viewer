{
  "name": "osm-notes-viewer",
  "version": "1.0.0",
  "description": "Web viewer for OpenStreetMap Notes analytics",
  "type": "module",
  "main": "src/index.html",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "serve": "python3 -m http.server 8000 --directory src",
    "test": "vitest",
    "test:run": "vitest --run",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "validate": "node scripts/validate-data.js",
    "validate:all": "node scripts/validate-data.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OSM-Notes/OSM-Notes-Viewer.git"
  },
  "keywords": [
    "openstreetmap",
    "osm",
    "notes",
    "analytics",
    "visualization",
    "statistics"
  ],
  "author": "Andres Gomez (AngocA)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/OSM-Notes/OSM-Notes-Viewer/issues"
  },
  "homepage": "https://github.com/OSM-Notes/OSM-Notes-Viewer#readme",
  "devDependencies": {
    "@vitest/coverage-v8": "^1.3.1",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "jsdom": "^22.1.0",
    "terser": "^5.44.0",
    "vite": "^4.5.14",
    "vitest": "^1.3.1"
  },
  "dependencies": {
    "chart.js": "^4.5.1",
    "leaflet": "^1.9.4"
  }
}
